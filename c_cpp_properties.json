{
    "version": 4,
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/build/include",
                "/usr/include",
                "/usr/local/include"
            ],
            "defines": [
                "_GNU_SOURCE",
                "_LARGE_FILES",
                "_LARGEFILE_SOURCE",
                "_FILE_OFFSET_BITS=64",
                "_TIME_BITS=64",
                "EV_COMPAT3=0",
                "PRODUCTION",
                "SSLSPLITTER",
                "WITH_TIMEBOMB",
                "WITH_JEMALLOC",
                "WITH_FASTOUT",
                "__KERNEL__",
                "MODULE",
                "KBUILD_MODNAME=\"fw\""
            ],
            "intelliSenseMode": "linux-gcc-x64",
            "forcedInclude": [
                "${workspaceFolder}/src/pch.h"
            ],
            "browse": {
                "databaseFilename": "",
                "limitSymbolsToIncludedHeaders": true
            },
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++20",
            "mergeConfigurations": true,
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ]
}